<div class="bumper row">
  <div class="col-lg-4"></div>
  <div class="col-lg-4">
    <h3>Edit Profile</h3>
    <form action=<%= $prefix + "/" + @apitoken + "/edit_profile/submit" %> method="POST">
      <div class="form-group">
  <label for="name">Name</label>
  <input type="text", name="profile[name]" id="name" placeholder="Alan Turing" class="form-control" />
      </div>
      <div class="form-group">
  <label for="dob">Birthday</label>
  <input type="date" name="profile[dob]" id="dob" class="form-control"/>
      </div>
      <div class="form-group">
  <label for="loc">Location</label>
  <input type="text" name="profile[location]" id="loc" placeholder="Boston" class="form-control" />
      </div>
      <div class="form-group">
  <label for="bio">Bio</label>
  <textarea class="form-control" id="bio" name="profile[bio]" placeholder="Tell us something about yourself..." rows="3"></textarea>
      </div>
      <input type="submit" value="Save" class="btn btn-primary" />
    </form>
    <div class="col-lg-4"></div>
  </div>
</div>
